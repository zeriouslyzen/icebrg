  "layout": {
    "layout_type": "flex",
    "direction": "column",
    "gap": "0",
    "padding": "0",
    "align_items": "stretch",
    "justify_content": "flex-start"
  },
  "theme": {
    "name": "chat_interface",
    "primary_color": "#3B82F6",
    "secondary_color": "#64748B",
    "background_color": "#FFFFFF",
    "text_color": "#1F2937",
    "font_family": "Inter, system-ui, sans-serif",
    "font_size_base": "16px",
    "border_radius": "12px",
    "shadow_base": "0 4px 6px rgba(0,0,0,0.1)"
  },
  "interactions": [
    {
      "event_type": "click",
      "handler_description": "Handle button clicks",
      "target_selector": "button"
    },
    {
      "event_type": "keydown",
      "handler_description": "Handle keyboard input",
      "target_selector": "input"
    },
    {
      "event_type": "submit",
      "handler_description": "Handle form submission",
      "target_selector": "form"
    }
  ],
  "accessibility_requirements": {
    "wcag_level": "AA",
    "min_contrast_ratio": 4.5,
    "keyboard_navigable": true,
    "screen_reader_compatible": true,
    "aria_labels_required": true,
    "focus_indicators": true,
    "alt_text_required": true,
    "semantic_html": true
  },
  "performance_budget": {
    "max_load_time_ms": 180,
    "max_bundle_size_kb": 80,
    "max_dom_nodes": 120,
    "max_reflows": 4,
    "max_repaints": 6,
    "cumulative_layout_shift": 0.06,
    "first_contentful_paint_ms": 130,
    "largest_contentful_paint_ms": 250
  },
  "security_contracts": [
    "no_external_scripts",
    "no_inline_eval",
    "sanitized_content",
    "csp_compliant",
    "no_dangerous_urls",
    "wcag_aa_compliant",
    "keyboard_navigable",
    "screen_reader_compatible",
    "has_aria_labels",
    "fast_load",
    "small_bundle",
    "no_layout_shift",
    "no_tracking",
    "no_third_party_embeds",
    "local_storage_only",
    "no_pii_exposure"
  ]
}
