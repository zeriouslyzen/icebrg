            "human_evaluators": 3,
            "success_criteria": 4.0,
            "target_success_rate": 90.0
          },
          "knowledge_integration_efficiency": {
            "definition": "Reduces data redundancy by 88% after integration compared to raw sources, measured by unique information content analysis",
            "efficiency_formula": "(1 - redundant_content/total_content) * 100",
            "target_efficiency": 88.0
          },
          "memory_retrieval_performance": {
            "simple_keyword_search": {
              "target_time": 50,
              "test_scenarios": 1000
            },
            "semantic_similarity_search": {
              "target_time": 100,
              "test_scenarios": 500
            },
            "cross_domain_pattern_search": {
              "target_time": 200,
              "test_scenarios": 200
            },
